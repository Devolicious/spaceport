imports:
    - { resource: config_dev.yml }

doctrine:
    dbal:
        host:     "mysql"

parameters:
  kunstmaan_search.hostname: "elasticsearch"

assetic:
    use_controller: true
    node: /usr/bin/node
    filters:
        uglifyjs2:
            bin: /usr/local/bin/uglifyjs
            compress: true
        uglifycss:
            bin: /usr/local/bin/uglifycss